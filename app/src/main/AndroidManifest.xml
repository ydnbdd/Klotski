<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrv.huarongdao">

    <application
        android:allowBackup="true"
        android:icon="@drawable/game"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".begin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            android:label = "Main Activity"
            android:parentActivityName=".begin">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mrv.huarongdao.begin" />
        </activity>
        <activity android:name=".helper">
            android:label = "helper Activity"
            android:parentActivityName=".begin">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mrv.huarongdao.begin" />
        </activity>
        <activity android:name=".historyActivity">
            android:label = "history Activity"
            android:parentActivityName=".begin">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mrv.huarongdao.begin" />
        </activity>
    </application>

</manifest>